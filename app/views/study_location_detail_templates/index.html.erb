<div class="row mb-3">
  <div class="col-md-12">
    <h1>
      All study location details
    </h1>

    <a href="/study_location_details/new" class="btn btn-block btn-outline-secondary">
      Add a new study location detail
    </a>
  </div>
</div>


<hr>

<div class="row mb-3">
  <div class="col-md-12">
    <table class="table">
      <tr>
        <th>
          ID
        </th>

        <th>
          Ambience
        </th>

        <th>
          Noise level
        </th>

        <th>
          Internet quality
        </th>

        <th>
          Plug availability
        </th>

        <th>
          Neighborhood
        </th>

        <th>
          User
        </th>

        <th>
          Address
        </th>

        <th>
          Created at
        </th>

        <th>
          Updated at
        </th>

        <th>
        </th>
      </tr>

      <% @study_location_details.each do |study_location_detail| %>
      <tr>
        <td>
          <%= study_location_detail.id %>
        </td>

        <td>
          <%= study_location_detail.ambience_id %>
        </td>

        <td>
          <%= study_location_detail.noise_level_id %>
        </td>

        <td>
          <%= study_location_detail.internet_quality_id %>
        </td>

        <td>
          <%= study_location_detail.plug_availability_id %>
        </td>

        <td>
          <%= study_location_detail.neighborhood_id %>
        </td>

        <td>
          <%= study_location_detail.user_id %>
        </td>

        <td>
          <%= study_location_detail.address_id %>
        </td>

        <td>
          <%= time_ago_in_words(study_location_detail.created_at) %> ago
        </td>
        <td>
          <%= time_ago_in_words(study_location_detail.updated_at) %> ago
        </td>

        <td>
          <a href="/study_location_details/<%= study_location_detail.id %>">
            Show details
          </a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
